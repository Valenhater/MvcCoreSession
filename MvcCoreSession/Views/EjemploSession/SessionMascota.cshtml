@{
    Mascota mascota = ViewData["MASCOTA"] as Mascota;
}

<h1 style="color:purple">Session Binary Object</h1>

<h2>@ViewData["MENSAJE"]</h2>


<p>
    <a asp-controller="EjemploSession" asp-action="SessionMascota" asp-route-accion="almacenar">Almacenar</a>
</p>
<p>
    <a asp-controller="EjemploSession" asp-action="SessionMascota" asp-route-accion="mostrar">Mostrar</a>
</p>

@if(mascota != null)
{
    <h1 style="color:red">Nombre: @mascota.Nombre, @mascota.Raza</h1>

    <h1 style="color:blue">Edad: @mascota.Edad</h1>
}